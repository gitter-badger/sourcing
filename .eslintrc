{
  "extends": [
    "eslint:recommended"
  ],
  "env": {
    "node": true,
    "es6": true
  },
  "ecmaFeatures": {
    "modules": true
  },
  "rules": {
    "arrow-spacing": 2,
    "constructor-super": 2,
    "dot-location": [2, "object"],
    "generator-star-spacing": 2,
    "no-class-assign": 2,
    "no-const-assign": 2,
    "no-extra-parens": 2,
    "no-else-return": 2,
    "no-this-before-super": 2,
    "no-extend-native": 2,
    "no-throw-literal": 2,
    "no-var": 2,
    "object-shorthand": 2,
    "prefer-const": 2,
    "prefer-spread": 2,
    "prefer-reflect": 2,
    "require-yield": 2
  }
}
